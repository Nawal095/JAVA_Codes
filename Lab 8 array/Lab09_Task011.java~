import java.util.Scanner;
public class Lab09_Task011 {
    public static void main(String[] args){
        Scanner input = new Scanner (System.in);
        
        int a[]=new int[5];
        for(int i=0; i<=a.length-1; i++)
        {
          a[i]=input.sc.nextInt();
        }
        
        largest=a[0];
        for(int i=1;i<=a.length-1;i++)
        {
          if(a[i]>largest)
          {
          largest=a[i];
          }
        }
    }
        