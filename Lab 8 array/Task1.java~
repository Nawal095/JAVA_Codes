import java.util.Scanner;
public class Task1{
  public static void main(String args[])
  {
    int[] arr = new int[3];
    Scanner sc = new Scanner(System.in);
    int sum =0;
    
    for(int i =0; i<3; i++)
    {
      arr[i]= sc.nextInt();
    }
    
    for(int i =0; i<3; i++)
    {
      
      sum = sum + arr[i];
    }
    
    System.out.print(sum+" ");
    
    for(int i =0; i<3; i++)
    {
      
      System.out.print(arr[i]+" ");
    }
  }
}